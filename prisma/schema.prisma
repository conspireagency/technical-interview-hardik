// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Car{
  id Int @id @default(autoincrement())
  year String @db.VarChar(50)
  make String @db.VarChar(225)
  model String @db.VarChar(225)
  submodel String @db.VarChar(225)
  notes String @db.VarChar(225)
  pL    String @db.VarChar(225)
  sizecode String @db.VarChar(225)
}